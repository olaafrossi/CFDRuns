FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      total(p);
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1500;

boundaryField
{
    symWall
    {
        type            symmetry;
    }
    outlet
    {
        type            calculated;
        value           uniform 1500;
    }
    upperWall
    {
        type            calculated;
        value           uniform 1500;
    }
    inlet
    {
        type            calculated;
        value           uniform 1500;
    }
    left
    {
        type            calculated;
        value           uniform 1500;
    }
    lowerWall
    {
        type            calculated;
        value           uniform 1500;
    }
    Wing_wall
    {
        type            calculated;
        value           uniform 0;
    }
    Endplate_wall
    {
        type            calculated;
        value           uniform 0;
    }
    Splitter_wall
    {
        type            calculated;
        value           uniform 0;
    }
}


