FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      MRFProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

MRF1
{
    cellZone    rotatingZoneFront;
    selectionMode cellZone;
    active      yes;

    // Fixed patches (by default they 'move' with the MRF zone)
    nonRotatingPatches ();

        origin    (0.0 0.0 0.0); //-0.965 0.290 1.01
        axis      (0 1 0);
        omega     constant -39.99; //-172.413;
}


