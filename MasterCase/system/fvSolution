FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        relTol 0.01;
        cacheAgglomeration on;
        nPreSweeps 1;
        agglomerator faceAreaPair;
        nCellsInCoarsestLevel 10;
        solver GAMG;
        nPostSweeps 1;
        mergeLevels 1;
        smoother GaussSeidel;
        tolerance 1e-08;
    }

    Phi
    {
        $p;
    }

    U
    {
        relTol 0.01;
        smoother symGaussSeidel;
        tolerance 1e-07;
        solver smoothSolver;
    }

    k
    {
        relTol 0.01;
        smoother symGaussSeidel;
        tolerance 1e-07;
        solver smoothSolver;
    }

    omega
    {
        relTol 0.01;
        smoother symGaussSeidel;
        tolerance 1e-07;
        solver smoothSolver;
    }
}

SIMPLE 
{
    pRefValue 0.0;
    residualControl 
    {
        k 1e-05;
        U 1e-05;
        "p.*" 1e-05;
        omega 1e-05;
    }
    nNonOrthogonalCorrectors 0;
    pRefCell 0;
}

potentialFlow
{
    pRefValue 0.0;
    nNonOrthogonalCorrectors 10;
    pRefCell 0;
}

relaxationFactors 
{
    fields 
    {
        p 0.2;
    }
    equations 
    {
        omega 0.5;
        k 0.5;
        U 0.5;
    }
